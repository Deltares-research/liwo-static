    "@vue/test-utils": "^2.4.5",
    "chai-things": "^0.2.0",
    "cypress": "^14.3.1",
    "eslint": "^9.24.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "^10.0.0",
    "happy-dom": "^17.4.4",
    "imports-loader": "^5.0.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "postcss-import": "^16.1.0",
    "sass": "^1.86.3",
    "sass-loader": "^16.0.5",
