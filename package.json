{
  "name": "liwo-static",
  "version": "3.9.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest --dom",
    "test:e2e": "cypress run",
    "test:long-running": "E2E_MODE=long-running vue-cli-service test:e2e",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "docker": "docker run --rm -p8080:80 --name liwo-static -v $(pwd)/nginx.conf:/etc/nginx/nginx.conf:ro -v $(pwd)/dist:/usr/share/nginx/html nginx",
    "release": "semantic-release"
  },
  "dependencies": {
    "@semantic-release/git": "^10.0.1",
    "@vue-a11y/skip-to": "^3.0.3",
    "dom-to-image": "^2.6.0",
    "fast-equals": "^5.0.1",
    "file-saver": "^2.0.5",
    "focus-trap": "^7.5.4",
    "leaflet": "^1.9.4",
    "leaflet-control-geocoder": "^2.4.0",
    "leaflet-easyprint": "^2.1.9",
    "leaflet.browser.print": "^2.0.2",
    "leaflet.markercluster": "^1.5.3",
    "lodash": "^4.17.21",
    "portal-vue": "^3.0.0",
    "proj4leaflet": "^1.0.2",
    "query-string": "^9.0.0",
    "tiny-cookie": "^2.5.1",
    "vue": "^3.4.21",
    "vue-cookie-law": "^1.13.3",
    "vue-matomo": "^4.2.0",
    "vue-router": "^4.3.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@cypress/skip-test": "^2.6.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "chai-things": "^0.2.0",
    "cypress": "^13.7.2",
    "eslint": "^8.56.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.26.0",
    "happy-dom": "^14.7.1",
    "imports-loader": "^5.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "postcss-import": "^16.1.0",
    "sass": "^1.74.1",
    "sass-loader": "^14.1.1",
    "semantic-release": "^19.0.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  }
